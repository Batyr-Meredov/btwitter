SELECT sentiment, created, count(1) FROM summary_2021_01 WHERE created='2021-01-01' GROUP BY sentiment, created ALLOW FILTERING USING TIMEOUT 240s;

 sentiment | created    | country | count
-----------+------------+---------+---------
  positive | 2021-01-01 |         |  371966
  negative | 2021-01-01 |         | 1179204
   neutral | 2021-01-01 |         | 1146081


cqlsh:twitter> select sentiment, created, language, country,count(1) from summary where language='en' and country in ('US', '')  group by sentiment, created, country allow filtering;

 sentiment | created    | language | country | count
-----------+------------+----------+---------+--------
   neutral | 2022-03-31 |       en |         | 497407
   neutral | 2022-03-31 |       en |      US |    693
